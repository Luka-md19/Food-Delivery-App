{
  "name": "food-delivery-app",
  "version": "0.0.1",
  "description": "Food delivery application",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.6.3",
  "scripts": {
    "build": "nest build && node build-worker-threads.js",
    "build:menu": "nest build menu && node build-worker-threads.js",
    "build:auth": "nest build auth && node build-worker-threads.js",
    "build:common": "nest build common",
    "build:worker-threads": "node build-worker-threads.js",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/food-delivery-app/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/food-delivery-app/test/jest-e2e.json",
    "typeorm": "pnpm exec ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "optimize-resources": "node optimize-resources.js",
    "restore-optimization": "node restore-optimization.js",
    "load-test:auth": "cd apps/auth/test/load-test/commands && run-all-tests.bat",
    "load-test:auth:baseline": "cd apps/auth/test/load-test/commands && run-baseline-test.bat",
    "load-test:auth:stress": "cd apps/auth/test/load-test/commands && run-stress-test.bat",
    "load-test:auth:realistic": "cd apps/auth/test/load-test/commands && run-realistic-test.bat",
    "load-test:menu": "cd apps/menu/test/load-test/commands && run-all-tests.bat",
    "load-test:menu:smoke": "cd apps/menu/test/load-test && k6 run smoke-test.js",
    "load-test:menu:baseline": "cd apps/menu/test/load-test && k6 run baseline-test.js",
    "load-test:menu:stress": "cd apps/menu/test/load-test && k6 run stress-test.js",
    "load-test:menu:spike": "cd apps/menu/test/load-test && k6 run spike-test.js",
    "load-test:menu:endurance": "cd apps/menu/test/load-test && k6 run endurance-test.js",
    "load-test:menu:realistic": "cd apps/menu/test/load-test && k6 run realistic-test.js",
    "load-test:menu:realistic-auth": "cd apps/menu/test/load-test/commands && run-realistic-auth-test.bat",
    "load-test:menu:realistic-auth:unix": "cd apps/menu/test/load-test/commands && sh run-realistic-auth-test.sh",
    "load-test:run": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/run-load-tests.ps1",
    "load-test:auth:run": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/run-load-tests.ps1 auth",
    "load-test:menu:run": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/run-load-tests.ps1 menu",
    "load-test:auth:all": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/run-load-tests.ps1 auth all",
    "load-test:menu:all": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/run-load-tests.ps1 menu all",
    "load-test:cleanup": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/cleanup-load-testing.ps1",
    "docker:enable-rate-limit:auth": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/docker-enable-rate-limiting.ps1 auth",
    "docker:disable-rate-limit:auth": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/docker-disable-rate-limiting.ps1 auth",
    "docker:enable-rate-limit:menu": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/docker-enable-rate-limiting.ps1 menu",
    "docker:disable-rate-limit:menu": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/docker-disable-rate-limiting.ps1 menu",
    "rate-limit:enable:all": "pnpm run docker:enable-rate-limit:auth && pnpm run docker:enable-rate-limit:menu",
    "rate-limit:disable:all": "pnpm run docker:disable-rate-limit:auth && pnpm run docker:disable-rate-limit:menu",
    "optimize:load-test": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/optimize-for-load-test.ps1",
    "optimize:load-test:restore": "docker-compose restart",
    "generate-report": "pnpm run generate-report:menu:baseline",
    "generate-report:menu:baseline": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/generate-test-report.ps1 -service menu -testType baseline",
    "generate-report:menu:stress": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/generate-test-report.ps1 -service menu -testType stress",
    "generate-report:menu:smoke": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/generate-test-report.ps1 -service menu -testType smoke",
    "generate-report:auth:stress": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/generate-test-report.ps1 -service auth -testType stress",
    "generate-report:auth:baseline": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/generate-test-report.ps1 -service auth -testType baseline",
    "generate-report:auth:realistic": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/generate-test-report.ps1 -service auth -testType realistic",
    "generate-reports": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/generate-html-reports.ps1",
    "generate-reports:menu": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/generate-html-reports.ps1 menu",
    "generate-reports:auth": "pnpm exec powershell -ExecutionPolicy Bypass -File ./scripts/generate-html-reports.ps1 auth",
    "load-test:menu:report:stress": "pnpm run generate-report:menu:stress",
    "load-test:menu:report:baseline": "pnpm run generate-report:menu:baseline",
    "load-test:menu:report:smoke": "pnpm run generate-report:menu:smoke",
    "load-test:auth:report:stress": "pnpm run generate-report:auth:stress",
    "load-test:auth:report:baseline": "pnpm run generate-report:auth:baseline",
    "load-test:auth:report:realistic": "pnpm run generate-report:auth:realistic"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.1.0",
    "@nestjs/cqrs": "^11.0.2",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.0",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.2",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cache-manager": "^5.0.0",
    "@types/cache-manager-redis-store": "^3.0.0",
    "@types/compression": "^1.7.5",
    "@types/helmet": "^4.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "cache-manager": "^6.4.2",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "express": "^4.21.2",
    "google-auth-library": "^9.15.1",
    "helmet": "^8.1.0",
    "html-minifier": "^4.0.0",
    "html-minifier-terser": "^7.2.0",
    "ioredis": "^5.5.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.1.0",
    "mongodb": "^6.14.0",
    "mongoose": "^8.11.0",
    "multer": "^2.0.0",
    "nest-winston": "^1.10.2",
    "nestjs-throttler-storage-redis": "^0.5.1",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.0.2",
    "@nestjs/testing": "^11.1.0",
    "@types/amqp-connection-manager": "^3.4.5",
    "@types/amqplib": "^0.10.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.17.14",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.7.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  },
  "pnpm": {
    "overrides": {
      "html-minifier": "npm:html-minifier-terser@7.2.0"
    }
  }
}